<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<% if user_signed_in? %>
<%= link_to "Log Out", destroy_user_session_path, method: :delete %>
<%  else %>
<%= link_to "Log In", new_user_session_path, remote: true, "data-toggle" => "modal", "data-target" => "#myModal" %>
<% end %>


<a href="/users/auth/facebook">Login with Facebook</a>
<a href="/users/auth/twitter">Login with Twitter</a>
<a href="/users/auth/google_oauth2">Login with Google</a>
<a href="/users/auth/linkedin">Login with Linked IN</a>

<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
</div>

<div class="modal fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
</div>
<div class="modal fade" id="myModal3" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
</div>

