<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<% if user_signed_in? %>
<%= link_to "Log Out", destroy_user_session_path, method: :delete %>
<%  else %>
<%= link_to "Log In", new_user_session_path, remote: true, "data-toggle" => "modal", "data-target" => "#myModal" %>
<% end %>

<div class="row">
    <div class="col-md-1">
    <a href="/users/auth/facebook"><%= image_tag "facebook.png", class: "img-responsive", style: "width:100%;height:50px;"%></a>
    </div>
    <div class="col-md-1">
    <a href="/users/auth/twitter"><%= image_tag "twitter.png", class: "img-responsive", style: "width:50px;height:50px;"%></a>
    </div>
    <div class="col-md-1">
    <a href="/users/auth/google_oauth2"><%= image_tag "google-plus.png", class: "img-responsive", style: "width:50px;height:50px;"%></a>
    </div>
    <div class="col-md-1">
    <a href="/users/auth/linkedin"><%= image_tag "linkedin.png", class: "img-responsive", style: "width:50px;height:50px;"%></a>
    </div>
</div>
<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
</div>

<div class="modal fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
</div>
<div class="modal fade" id="myModal3" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
</div>

<style>
    .modal-sign-up {
      height:500px;
      overflow:auto;
  }
</style>

